{
    "manifest_version": 3,
    "name": "Note X",
    "version": "1.0",
    "description": "A note-taking extension for research and online classes",
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    }
  }
  
